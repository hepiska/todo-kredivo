{
  "name": "todo-be",
  "version": "1.0.0",
  "description": "simple todo list be",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require @babel/register",
    "build": "babel src --out-dir dist",
    "serve": "node dist/index.js",
    "start": "npm run build && npm run serve",
    "watch": "nodemon app.js --exec babel-node",
    "migrate-test": "cd ./src/db && NODE_ENV=test sequelize  db:migrate ",
    "seed-test": "cd ./src/db && NODE_ENV=test sequelize  db:seed:all "
  },
  "author": "hepiska",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "app-module-path": "^2.2.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "sequelize": "^5.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "prettier": "^1.15.3"
  }
}
